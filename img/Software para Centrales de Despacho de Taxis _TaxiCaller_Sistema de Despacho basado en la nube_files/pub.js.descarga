(function($){
	"use strict";

	var CountryCodes = function(form, options){
		this.$form = $(form),
		this.country_data = options.country_data,
		this.$country_select = options.country_select,
		this.$phone_input = options.phone_input,
		this.country_dict = {};
		this.init();
	};

	CountryCodes.prototype.init = function(){
        var self = this;
		this.populateCountrySelect()
		.addCountryCodeOnSelect();

        setTimeout( function() {
            self.getAndSelectUserCountry();
        }, 100);
	};

	CountryCodes.prototype.populateCountrySelect = function(){
		var self = this;
		$.each(this.country_data, function(i, country){
			var $o = $(document.createElement("option"));
			$o.attr("value", country.code);
			$o.html(country.name);
			self.country_dict[country.code] = country.ccode;
			self.$country_select.append($o);
		});
		return self;
	};

	CountryCodes.prototype.addCountryCodeOnSelect = function(){
		var self = this;
		this.$country_select.change(function(){
			var ccode = self.country_dict[$(this).val()];
			self.$phone_input.val("+"+ccode+ " ");
			self.addPhoneValidation(ccode);
		});
		return self;
	};

	CountryCodes.prototype.addPhoneValidation = function(ccode){
		//Remove old validations if existing
		this.$phone_input.off("input");
		var oldvalue = this.$phone_input.val();
		var pattern = "^\\+" + ccode + "[ \\(\\)\\d-\\./]*$";

		var regex = new RegExp(pattern);

		this.$phone_input.on("input", function(e){
			if ($(this).val() == ""){
				$(this).val("+" + ccode + " ");
				oldvalue = $(this).val();
			}
			else if (!regex.test($(this).val())){
				$(this).val(oldvalue);
			}
			else{
				oldvalue = $(this).val();
			}
		});
	};

	CountryCodes.prototype.getAndSelectUserCountry = function(){
		var self = this;

        $.getJSON('/geoprice.php', function(loc){
			var usercountry = loc.country_code.toLowerCase();
			self.$country_select.val(usercountry);
			self.$country_select.trigger("change");
		});
	};

	$.fn.countrycodes = function(options){
		return this.each(function(){
			$(this).data("countrycodes", new CountryCodes(this, options));
		});
	};

	$.fn.countrycodes.Constructor = CountryCodes;

})(jQuery);
